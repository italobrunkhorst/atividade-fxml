<?xml version="1.0" encoding="UTF-8"?>


<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>


<VBox xmlns="http://javafx.com/javafx" 
      xmlns:fx="http://javafx.com/fxml" 
      prefHeight="400" prefWidth="600"
      fx:controller="base.exemplos.fxml.controllers.TelaCadastro"
    >

    <Label text="Placa:"/>
    <TextField fx:id="tfPlaca" />

    <Label text="Modelo:"/>
    <TextField />

    <Label text="Cor:"/>
    <TextField />
    
    <Label text="Fabricante:"/>
    <TextField />

    <Label text="Ano Fabricação:"/>
    <TextField />

    <HBox spacing="5.0" alignment="CENTER_RIGHT">
        <Button text="Cancelar" />
        <Button text="Cadastrar" onAction="#cadastrar"/>
    </HBox>

</VBox>